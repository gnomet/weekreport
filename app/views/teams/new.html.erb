<h1>New team</h1>

<% form_for(@team) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= "Team name" %><br />
    <%= f.text_field :name %>
  </p>
  <p>
    <%= "Contact person" %><br />
    <%= f.text_field :contact_person %>
  </p>
  <p>
    <%= "Contact person email" %><br />
    <%= f.text_field :email %>
  </p>
  <p>
    <%= f.submit 'Create' %>
  </p>
<% end %>

<%= link_to 'Back', teams_path %>